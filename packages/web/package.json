{
	"name": "hledger-client-web",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"directories": {
		"public": "public",
		"types": "types",
		"tool": "tool",
		"build": "build",
		"src": "src",
		"test": "test"
	},
	"scripts": {
		"build": "NODE_ENV=production vite build",
		"build:analyze": "vite build --mode analyze",
		"serve": "vite build && vite preview --port 3050 --host",
		"clean": "rimraf .turbo node_modules build dist",
		"test:typecheck": "tsc --noEmit",
		"test:eslint": "eslint 'src/**/*.{ts,tsx}' --cache --cache-location node_modules/.cache/.eslintcache --cache-strategy content",
		"test:format": "prettier --write --ignore-unknown .",
		"test:stylelint": "stylelint \"src/**/*.{css,scss}\"",
		"test:circular": "madge --circular --extensions ts,tsx src",
		"test": "pnpm run /^test:/",
		"dev": "vite"
	},
	"dependencies": {
		"@solid-primitives/media": "2.2.9",
		"@solid-primitives/refs": "1.0.8",
		"@solid-primitives/utils": "6.2.3",
		"@solidjs/router": "0.14.9",
		"axios": "1.7.7",
		"loglevel": "1.9.2",
		"luxon": "3.5.0",
		"ramda": "0.30.1",
		"ramda-adjunct": "5.1.0",
		"solid-js": "1.9.2",
		"solid-presence": "0.1.8",
		"uuid": "10.0.0",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@solid-devtools/overlay": "0.30.1",
		"@types/jsdom": "21.1.7",
		"@types/luxon": "3.4.2",
		"@types/node": "22.8.1",
		"@types/ramda": "0.30.2",
		"@types/uuid": "10.0.0",
		"@typescript-eslint/eslint-plugin": "8.11.0",
		"@typescript-eslint/parser": "^8.11.0",
		"@vanilla-extract/vite-plugin": "4.0.16",
		"@vitejs/plugin-basic-ssl": "1.1.0",
		"autoprefixer": "10.4.20",
		"eslint": "9.13.0",
		"eslint-define-config": "2.1.0",
		"eslint-import-resolver-typescript": "3.6.3",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-jsdoc": "50.4.3",
		"eslint-plugin-n": "17.11.1",
		"eslint-plugin-node-import": "1.0.4",
		"eslint-plugin-perfectionist": "3.9.1",
		"eslint-plugin-prefer-arrow": "1.2.3",
		"eslint-plugin-prefer-let": "4.0.0",
		"eslint-plugin-promise": "7.1.0",
		"eslint-plugin-sonarjs": "2.0.4",
		"eslint-plugin-types": "1.0.2",
		"eslint-plugin-unicorn": "56.0.0",
		"globals": "15.11.0",
		"madge": "8.0.0",
		"postcss": "8.4.47",
		"postcss-100vh-fix": "1.0.2",
		"postcss-dark-theme-class": "1.3.0",
		"postcss-import": "16.1.0",
		"postcss-modules": "6.0.0",
		"postcss-normalize": "13.0.1",
		"postcss-preset-env": "10.0.7",
		"prettier": "3.3.3",
		"rimraf": "6.0.1",
		"rollup-plugin-visualizer": "5.12.0",
		"sass": "1.80.5",
